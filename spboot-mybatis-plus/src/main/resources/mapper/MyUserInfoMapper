<mapper namespace="com.wangye.spbootmybatisplus.mapper.MyUserInfoMapper">

    <select id="selectById" resultType="com.wangye.spbootmybatisplus.model.MyUserInfo">
        select
            id as id,
            name as name,
            age as age
        from
            my_user_info
        where
            id = #{id}
    </select>

    <select id="getPage" resultType="com.wangye.spbootmybatisplus.model.MyUserInfo">
        select
            id as id,
            name as name,
            age as age
        from
            my_user_info
    </select>

